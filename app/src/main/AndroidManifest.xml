<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.product.stepanenko.calculatemealday">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Registration"
            android:label="@string/registration" />
        <activity android:name=".MealActivity" />
        <activity android:name=".MealFavoriteActivity" />
        <activity android:name=".AddNewMealActivity" />
        <activity android:name=".EditMealActivity" />
        <activity android:name=".HistoryMeal" />
        <activity android:name=".Profile" />
        <activity android:name=".EditRegData"></activity>
    </application>

</manifest>